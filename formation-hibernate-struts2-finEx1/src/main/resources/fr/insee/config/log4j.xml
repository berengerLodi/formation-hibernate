<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE log4j:configuration PUBLIC "log4j.dtd" "http://logging.apache.org/log4j/1.2/apidocs/org/apache/log4j/xml/doc-files/log4j.dtd">
<log4j:configuration>

	<!-- Appenders -->
	<appender name="console" class="org.apache.log4j.ConsoleAppender">
		<layout class="org.apache.log4j.PatternLayout">
			<param name="ConversionPattern" value="%5p %d{HH:mm:ss,SSS} - %l - %m%n" />
		</layout>
	</appender>
	
	<!-- Loggers -->
	<logger name="fr.insee.tp">
		<level value="INFO" />
		<appender-ref ref="console" />
	</logger>
 	<logger name="com">
		<level value="WARN" />
		<appender-ref ref="console" />
	</logger>
	<logger name="org">
		<level value="WARN" />
		<appender-ref ref="console" />
	</logger>
</log4j:configuration>

